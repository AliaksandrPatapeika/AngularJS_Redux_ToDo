{
  "name": "angularjs_todo",
  "version": "0.0.1",
  "description": "A tutorial application for AngularJS",
  "author": "Aliaksandr Patapeika",
  "main": "src/app/app.js",
  "license": "ISC",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev npm run startDevServer",
    "start:prod": "cross-env NODE_ENV=production npm run startDevServer",
    "build": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=dev npm run buildDistFiles",
    "build:prod": "cross-env NODE_ENV=production npm run buildDistFiles",
    "test": "karma start",
    "startDevServer": "webpack-dev-server --inline --progress --compress",
    "buildDistFiles": "rimraf dist && webpack"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "angular-mocks": "1.6.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "6.4.1",
    "cross-env": "4.0.0",
    "css-loader": "2.1.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "jasmine-core": "2.5.2",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-webpack": "3.0.5",
    "ng-annotate-loader": "0.6.1",
    "rimraf": "2.6.3",
    "webpack": "2.7.0",
    "webpack-dev-server": "2.11.4"
  },
  "dependencies": {
    "angular": "1.6.4",
    "angular-animate": "1.6.4",
    "@uirouter/angularjs": "1.0.5",
    "ng-redux": "4.3.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6"
  }
}
